<form method="post" action="{{ path('app_skills_delete', {'id': skill.id}) }}" onsubmit="return confirm('Êtes-vous sûre de vouloir supprimer cet élément ?');">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ skill.id) }}">
	<button type="submit" class="btn">Supprimer</button>
</form>
